[nodes]
{{- range $nodepoolInfo := .NodepoolInfos }}
    {{- range $nodepool := $nodepoolInfo.Nodepools }}
        {{- range $node :=  $nodepool.Nodes }}
{{ trimPrefix $node.Name (printf "%s-" $nodepoolInfo.ID) }} ansible_host={{ $node.Public }} private_ip={{ $node.Private }} netmask={{ extractNetmaskFromCIDR $nodepoolInfo.Network }} ansible_ssh_private_key_file={{ $nodepoolInfo.ID }}.pem
        {{- end }}
    {{- end }}
{{- end }}

